{
  "name": "request-pre",
  "version": "0.1.0",
  "description": "process format data before request send",
  "scripts": {
    "test": "jest",
    "watch": "npm t -- --watch",
    "coverage": "jest --coverage",
    "lint": "eslint src --fix --ext .ts",
    "lib": "tsc",
    "dev": "tsc -w"
  },
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "keywords": [
    "axios",
    "pre",
    "request",
    "ajax"
  ],
  "author": "zhangsanshi",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix",
      "npm run test",
      "npm run lib",
      "git add"
    ]
  }
}
